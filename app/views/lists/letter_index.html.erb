<% cache(index_letter_cache_key(@elements, List, params[:letter])) do %>
  <%= render 'shared/index_header', :title => "Cocktail Lists Index: #{params[:letter].upcase}" %>
  <%= render 'shared/index_menu' %>
  <%= render 'shared/index_body', :controller => :lists, :elements => @elements %>
  <%= render 'shared/index_alphabet_menu', :controller => :lists, :title => 'Lists A-Z' %>
  <%= render 'shared/breadcrumbs_for_letter', :element => List.new, :letter => params[:letter] %>
<% end %>
