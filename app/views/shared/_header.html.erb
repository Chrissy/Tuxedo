<div class="header-content">
  <%= filepicker_image_tag element.image, w: 1880, h: 1080, fit: 'crop', crop: "0,0,1880,1080", align: "center,center" %>
  <h1 class="header-title">
    <a href="<%= element.url %>">
      <%= render "shared/display_number", :number => element.number if element.respond_to?(:number) %>
      <%= swash(element.name) %>
    </a>
  </h1>
</div>
<%= render 'admin/user_links' %>