<%= link_to element.url, class: "list-element-number" do %>
  <%= render "shared/display_number", :number => element.number if element.respond_to?(:number) %>
<% end %>
<h4 class="subtext">
  <% element.components[0..2].each do |component| %>
    <%= link_to component.nickname, component.url %>
  <% end %>
</h4>