{
  "name": "tuxedo",
  "version": "0.0.1",
  "engines": {
    "node": "6.9.4"
  },
  "scripts": {
    "start": "",
    "assets": "watchify -t [ babelify ] app/assets/javascripts/application.js -o public/dist/application.js",
    "cms": "watchify -t [ babelify ] app/assets/javascripts/cms.js -o public/dist/cms.js",
    "styles": "node-sass app/assets/stylesheets/application.scss public/dist/application.css",
    "dev": "npm run assets && npm run cms && node-sass app/assets/stylesheets/application.scss public/dist/application.css -w" 
  },
  "license": "ISC",
  "devDependencies": {
    "awesomplete": "^1.1.1",
    "aws-sdk": "^2.36.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "escape-string-regexp": "^1.0.5",
    "jquery": "^3.4.0",
    "node-sass": "^4.13.0",
    "watchify": "^3.7.0"
  }
}
